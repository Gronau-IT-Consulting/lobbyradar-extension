{
	"name": "Lobbyradar",
	"author": "mdt",
	"version": "0.1",
	"manifest_version": 2,
	"description": "Discover Lobby Networks",
	"background": {
		"scripts": [
			"preferences.js",
			"background.js",
			"dataUpdater.js",
			"names.json"
		]
	},
	"content_scripts": [
		{
			"matches": [
				"*://*/*"
			],
			"js": [
				"BabelExt.js",
				"lobbyradar.js"
			],
			"run_at": "document_idle"
		}
	],
	"icons": {},
	"permissions": [
		"*://*/*",
		"contextMenus",
		"tabs",
		"history",
		"notifications",
		"storage"
	],
	"options_page": "options.html"
}
